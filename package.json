  },
  "homepage": "https://github.com/rosnovsky/auth0-example-apps#readme",
  "private": true,
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.13.5",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.11",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/auth0": "^2.20.11",
    "@types/images": "^2.2.2",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.9.0",
    "babel-preset-react": "^6.24.1",
    "btoa": "1.2.1",
    "eslint": "^7.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.5",
    "gatsby": "2.29.1",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-generate-types": "^0.1.0",
    "gatsby-plugin-manifest": "2.9.0",
    "gatsby-plugin-netlify": "2.8.0",
    "gatsby-plugin-offline": "3.7.0",
    "gatsby-plugin-preload-link-crossorigin": "^1.0.2",
    "gatsby-plugin-react-helmet": "3.7.0",
    "gatsby-plugin-sharp": "2.11.1",
    "gatsby-plugin-typescript": "^2.9.0",
    "gatsby-source-filesystem": "2.8.0",
    "gatsby-source-sanity": "6.0.4",
    "gatsby-transformer-sharp": "2.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "netlify-plugin-gatsby-cache": "0.3.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-fontawesome": "^1.7.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "clean-css-cli": "^4.2.1",
    "cpx": "^1.5.0",
    "cypress": "^6.1.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "gatsby-cli": "^2.16.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^2.2.1",
    "pug": "^3.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "react-content-loader": "5.1.4",
    "react-helmet": "^6.0.0",
    "react-test-renderer": "^17.0.1",
    "reactstrap": "8.7.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "predeploy": "yarn format && yarn lint && yarn type-check",
    "postdeploy": "yarn badges",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "badges": "npx lighthouse-badges --urls https://dse.netlify.app -o badges",
    "css:browsersync": "browser-sync start --directory --server ./public --files ./public/* --startPath index.html",
    "css:assets": "cpx \"src/assets/**/*.*\" src/",
    "css:compile": "tailwind build src/tailwind/tailwind/tailwind.css -c tailwind.config.js -o src/tailwind/tailwind/tailwind-theme.css",
    "css:minify": "cleancss --level 1 --source-map --source-map-inline-sources --output src/tailwind-theme.min.css src/tailwind/tailwind/tailwind-theme.css",
    "css": "npm-run-all --sequential css-*",
    "css:templates": "pug --pretty -o public/ src/",
    "css:build": "npm-run-all --sequential clean copy-* templates css",
    "css:watch": "npm-run-all build --parallel watch-templates watch-css browsersync",
    "css:watch-templates": "npm run templates -- --watch",
    "css:watch-css": "npm run css-compile -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
