{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2019",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "strict": false,
    "noEmit": true,
    "sourceMap": true,
    "isolatedModules": false,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "allowJs": true,
  },
  "exclude": ["node_modules", "public"]
}
